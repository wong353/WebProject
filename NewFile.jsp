                next();
            });
        } else if (this.id === 's_2') {
            $('#if_2, #crsl_2').show().queue(function(next) {
                $('.cln_nm > span, .lyr#i_cln .top h6').text('fall/winter 2020 Editorial');
                $('.lyr .cln_nm, .lyr .info').addClass('on');
                $('#dsc_clos').show();
                fl('#crsl_2', '#if_2');
                m_total('#crsl_2');
                s_thumb('#if_2');
                next();
            });
        }/*else if (this.id === 's_mv') {
            $('#if_mv').show();
            $('#v_x').show();
            player.playVideo();
        }*/
    });
    
    $('#dsc_clos').click(function(e) {
        if ($(this).attr('data-state') == 1) {
            $('#dsc_clos .d2').show();
            $('.lyr .info.on .cln_nm').hide();
            $(this).attr('data-state', 0);
        } else {
            $('#dsc_clos .d2').hide();
            $('.lyr .info.on .cln_nm').show();
            $(this).attr('data-state', 1);
        }
        e.preventDefault();
    });
    
    $('#btn_play, #btn_mute').attr('data-state', 1);
    
    $('#btn_mute').click(function(e) {
        if ($(this).attr('data-state') == 1) {
            player.mute();
            $(this).text('Unmute');
            $(this).attr('data-state', 0);
        } else {
            player.unMute();
            $(this).text('Mute');
            $(this).attr('data-state', 1);
        }
        e.preventDefault();
    });
    
    $('#btn_play').click(function(e) {
        if ($(this).attr('data-state') == 1) {
            player.pauseVideo();
            $(this).text('Play');
            $(this).attr('data-state', 0);
        } else {
            player.playVideo();
            $(this).text('Pause');
            $(this).attr('data-state', 1);
        }
        e.preventDefault();
    });
}
    
$(window).load(function() {
    plyr();
});
</script>
</html>
